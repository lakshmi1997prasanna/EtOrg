<aura:component controller="AccountLighting" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction" access="global" >
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="listAppDocCat" type="Contact[]"/>
    <aura:attribute name="accept" type="List" default="['.jpg', '.jpeg', '.png']"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <div class="slds-box slds-theme_shade slds-theme_alert-texture">
        <aura:iteration items="{!v.listAppDocCat}" var="appdoc"> 
        <center>
                        <lightning:fileUpload  name="{!appdoc.Name}"
                           label= "{!appdoc.Name}"
                           multiple="false"
                           accept="{!v.accept}"                  
                           recordId="{!appdoc.Id}"
                           onuploadfinished="{!c.handleUploadFinished }"/> 
                    </center>
        </aura:iteration>
    </div>
</aura:component>